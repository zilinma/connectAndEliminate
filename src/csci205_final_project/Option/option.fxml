<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="285.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="csci205_final_project.Option.OptionController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Option">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Region VBox.vgrow="ALWAYS" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Volume" />
      <Region VBox.vgrow="ALWAYS" />
      <Slider fx:id="volumeSlider" />
      <Region VBox.vgrow="ALWAYS" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Themes" />
      <ComboBox fx:id="themeBox" prefWidth="150.0" />
      <Region VBox.vgrow="ALWAYS" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Level" />
      <ComboBox fx:id="levelBox" prefWidth="150.0" />
      <Region />
      <Button fx:id="startGame" mnemonicParsing="false" onAction="#start" prefHeight="63.0" prefWidth="150.0" text="Start" />
      <Button fx:id="returnBtn" alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" onAction="#resume" text="Return" />
      <Region VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</VBox>
