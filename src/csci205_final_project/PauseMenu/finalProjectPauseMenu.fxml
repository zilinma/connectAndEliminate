<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="pauseVBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="csci205_final_project.PauseMenu.FinalProjectPauseMenuController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Pause Menu">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Button fx:id="resumeBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#resumeGame" text="Resume" />
      <Button fx:id="saveBtn" mnemonicParsing="false" onAction="#saveGame" text="Save Game" />
      <Button fx:id="loadBtn" mnemonicParsing="false" onAction="#loadGame" text="Load Game" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
